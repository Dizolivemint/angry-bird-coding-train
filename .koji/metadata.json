{
  "metadata": {
    "title": "Koji p5.js Scaffold",
    "description": "Build and deploy your p5.js applications quickly with this scaffold.",
    "image": "https://images.koji-cdn.com/be061740-660e-475b-b943-3f560db3a57f/Screenshot from 2019-04-22 18-27-44.png",
    "icon": "https://images.koji-cdn.com/be061740-660e-475b-b943-3f560db3a57f/86d7f02c-06f2-42aa-93c4-2df573590595.png",
    "gaCode": ""
  },
  "@@editor": [
    {
      "key": "metadata",
      "name": "Metadata",
      "icon": "âš™",
      "source": "metadata.json",
      "fields": [
        {
          "key": "title",
          "name": "Name for your App",
          "description": "Will show up as title on Google and Facebook"
        },
        {
          "key": "description",
          "name": "Description",
          "description": "Description of your app for Google and Facebook",
          "type": "textarea"
        },
        {
          "key": "gaCode",
          "name": "GA Tracking ID",
          "description": "Tracking ID for Google Analytics",
          "type": "text",
          "typeOptions": {
            "monospace": true
          }
        },
        {
          "key": "image",
          "name": "Share Image",
          "type": "image",
          "description": "Embedded Image to load when your App is shared"
        },
        {
          "key": "icon",
          "name": "Icon",
          "type": "image",
          "description": "Small Icon for your App"
        }
      ]
    }
  ]
}